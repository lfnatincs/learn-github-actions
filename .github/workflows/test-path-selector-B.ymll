name: Test Path Selector Project B

on:
  workflow_dispatch:
  push:
    branches: [main]
    paths:
      - "!projectA/*"

permissions:
  contents: read

jobs:
  command-execute:
    runs-on: ubuntu-latest

    steps:
      - name: checkout
        uses: actions/checkout@v3

      - name: command
        run: |
          bash ${{github.workspace}}/projectB/projectB.sh
